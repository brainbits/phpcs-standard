{
    "name": "brainbits/phpcs-standard",
    "description": "Brainbits Coding Style",
    "license": "MIT",
    "type": "phpcodesniffer-standard",
    "autoload": {
        "psr-4": {
            "BrainbitsCodingStandard\\": "BrainbitsCodingStandard"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrainbitsCodingStandard\\": "tests"
        }
    },
    "require": {
        "squizlabs/php_codesniffer": "!=3.2.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.2",
        "slevomat/coding-standard": "^4.0",
        "escapestudios/symfony2-coding-standard": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
