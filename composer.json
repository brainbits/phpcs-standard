{
    "name": "brainbits/phpcs-standard",
    "description": "Brainbits Coding Style",
    "keywords": ["brainbits", "coding", "standard", "cs", "code", "style", "sniffer"],
    "license": "MIT",
    "type": "phpcodesniffer-standard",
    "authors": [
        {"name": "Stephan Wentz", "email": "swentz@brainbits.net"}
    ],
    "autoload": {
        "psr-4": {
            "BrainbitsCodingStandard\\": "BrainbitsCodingStandard"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrainbitsCodingStandard\\": "tests"
        }
    },
    "require": {
        "squizlabs/php_codesniffer": "^3.3.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "slevomat/coding-standard": "^5.0.0",
        "escapestudios/symfony2-coding-standard": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
